pipeline {
libraries{
lib 'shlib'
}

 agent any
   tools{
        maven "Maven"
    }
    stages {
    stage('git repolist')
    {
    steps
    {
    gitrepolist()
    logfunction("collected repos list succesfully")
    }
    post{
    failure{
    logfunction("repo list is not collected")
    }}}
   stage('git repo')
    {
    steps
    {
    gitrepo()
    logfunction("created repo succesfully")
    }
    post{
    failure{
    logfunction("repo list is not collected")
    }}}
    
    
    
    stage('git branch')
    {
    steps
    {
    gitbranchlist()
    }
    }
    
    stage('gitlab repolist ')
    {
    steps
    {
    collectgitlabrepos()
    }
    }
    
    stage('gitlab repo')
    {
    steps
    {
    createrepogitlab()
    }
    }
    stage('gitlab branch list')
    {
    steps
    {
    gitlabbranchlist()
    }
    }
    
    stage('gitlab branch')
    {
    steps
    {
    gitlabbranch()
    }
    }
}
}

