pipeline {
libraries{
lib 'shlib'
}

 agent any
   tools{
        maven "Maven"
    }
    stages {
    stage('git repolist')
    {
    steps
    {
    gitrepolist()
    log_github("collected repo list succesfully")
    }
    post{
    failure{
    log_github("repo list is not collected")
    }}}
   stage('git repo')
    {
    steps
    {
    gitrepo()
    log_github("created  succesfully")
    }
    post{
    failure{
    log_github("not created")
    }}}
    
    
    
    stage('git branch')
    {
    steps
    {
    gitbranchlist()
    }
    }
    
    stage('gitlab repolist ')
    {
    steps
    {
    gitlabrepolist()
    log_gitlab("collected project list succesfully")
    }
    post{
    failure{
    log_gitlab("project list is not collected")
    }}}
    
    
    
    stage('gitlab repo')
    {
    steps
    {
    createrepogitlab()
     log_gitlab("created succesfully")
    }
    post{
    failure{
    log_gitlab("project list is not collected")
    }}}
    
    
    stage('gitlab branch list')
    {
    steps
    {
    gitlabbranchlist()
    }
    }
    
   /* stage('gitlab branch')
    {
    steps
    {
    gitlabbranch()
    }
    }*/
}
}

