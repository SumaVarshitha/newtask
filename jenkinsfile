pipeline {
libraries{
lib 'shlib'
}

 agent any
   tools{
        maven "Maven"
    }
    stages {
   
    stage('git repolist')
    {
    steps
    {
    gitrepolist()
    log_github("collected repo list succesfully")
    }
    post{
    failure{
    log_github("repo list is not collected")
    }}}
    
    stage('git repo')
    {
    steps
    {
    gitrepo(JSON)
    log_github("created  succesfully")
    }
    post{
    failure{
    log_github("not created")
    }}}
    
    stage('git branch list')
    {
    steps
    {
    gitbranchlist(JSON)
    }
    }
    
    stage('git branch')
    {
    steps
    {
    gitbranch(JSON)
    }
    }
    
    stage('github branch deletion')
    {
    steps
    {
    deletebranch(JSON)
    }
    }
    
    
    stage('github repo deletion')
    {
    steps
    {
    deleterepo(JSON)
    }
    }
    
    
    
    
    
    
     stage('githubcommit')
    {
    steps
    {
    githubcommit(JSON)
    }
    }
    
    stage('gitlab repolist ')
    {
    steps
    {
    gitlabrepolist()
    log_gitlab("collected project list succesfully")
    }
    post{
    failure{
    log_gitlab("project list is not collected")
    }}}
    
    stage('gitlab repo')
    {
    steps
    {
    createrepogitlab(JSON)
     log_gitlab("created succesfully")
    }
    post{
    failure{
    log_gitlab("project list is not collected")
    }}}
    
    stage('gitlab branch list')
    {
    steps
    {
    gitlabbranchlist()
    }
    }
    
    stage('gitlab branch')
    {
    steps
    {
    gitlabbranch(JSON)
    }
    }
    
   
    
    stage('gitlab branch deletion')
    {
    steps
    {
    branchrmvgitlab(JSON)
    }
    } 
    
    stage('gitlab project deletion')
    {
    steps
    {
    removegitlab(JSON)
    }
    }
    
    stage('import')
    {
    steps
    {
    importgithub()
    }
    }
    
    stage('commit')
    {
    steps
    {
    commitgitlab()
    }
    }
   
   
}
}
    
    
   
    
    
    
   
     
    
    
    
    
    
    
    
    
    
    
    

